{
  "name": "fast-fuzz",
  "version": "2.2.0",
  "description": "Test case generator based on branch coverage and fast-check.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist build package",
    "build-all": "yarn clean && yarn install",
    "lint": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "postinstall": "node ./postinstall.js",
    "test": "mocha",
    "publish": "node publish.js"
  },
  "keywords": [
    "testing",
    "white-box",
    "unit-testing",
    "typescript",
    "fuzzing",
    "generative-testing",
    "verification"
  ],
  "author": "Adrian Burlacu <adrian.burlacu@live.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeWatchWall/fast-fuzz.git"
  },
  "dependencies": {
    "deep-object-diff": "^1.1.0",
    "fast-check": "^2.17.0",
    "fast-copy": "^2.1.1",
    "istanbul-lib-hook": "file:./istanbuljs/istanbul-lib-hook",
    "istanbul-lib-instrument": "file:./istanbuljs/istanbul-lib-instrument"
  },
  "devDependencies": {
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "latest-version": "^6.0.0",
    "mocha": "^9.1.2",
    "objectmodel": "^4.3.0",
    "typescript": "^4.1.3"
  }
}
