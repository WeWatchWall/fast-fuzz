{
  "name": "fast-fuzz",
  "version": "4.1.0",
  "description": "Test case generator based on branch coverage and fast-check.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "fast-fuzz": "dist/src/bin.js"
  },
  "scripts": {
    "clean": "node clean.js dist build package",
    "lint": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "fix": "eslint ./ --ext .js,.jsx,.ts,.tsx --fix",
    "build": "tsc",
    "build-all": "npm run clean && npm run build",
    "postinstall": "npm run build-all",
    "test": "ts-mocha test/**/*.test.ts",
    "updatePublish": "node publish.js"
  },
  "keywords": [
    "testing",
    "white-box",
    "unit-testing",
    "typescript",
    "fuzzing",
    "generative-testing",
    "verification"
  ],
  "author": "Adrian Burlacu <adrian.burlacu@live.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeWatchWall/fast-fuzz.git"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "flat-promise": "^1.0.3",
    "latest-version": "^6.0.0",
    "mocha": "^9.1.2",
    "objectmodel": "^4.3.0",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "commander": "^6.2.1",
    "deep-object-diff": "^1.1.0",
    "fast-copy": "^2.1.1",
    "fast-safe-stringify": "^2.1.1",
    "glob": "^8.0.1",
    "intermock": "^0.2.5",
    "istanbul-lib-hook": "^3.0.0",
    "istanbul-lib-instrument": "^5.2.0",
    "make-matrix": "^1.3.1",
    "multee": "^0.2.4",
    "reflect-metadata": "^0.1.X",
    "tplant": "github:WeWatchWall/tplant"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.X",
    "typescript": "^4.X.X"
  }
}
